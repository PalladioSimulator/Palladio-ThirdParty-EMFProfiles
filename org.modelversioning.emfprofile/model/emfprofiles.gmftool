          title="EClass"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Generalization"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Reference"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Package"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Enumeration"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="DataType"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Literal"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Attribute"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Operation"/>
    </tools>
  </palette>
</gmftool:ToolRegistry>
