<?xml version="1.0" encoding="UTF-8"?>
<gmftool:ToolRegistry xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <palette
      title="emfprofilePalette">
    <tools
        xsi:type="gmftool:ToolGroup"
        title="EMFprofile"
        collapsible="true">
      <tools
          xsi:type="gmftool:CreationTool"
          title="Stereotype"
          description="Create new Stereotype"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Tagged Value"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Extension"
          description="Create new Extension Relationship">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="icons\full\obj16\Extension.gif"
            bundle="org.modelversioning.emfprofile.edit"/>
        <largeIcon
            xsi:type="gmftool:BundleImage"
            path="icons\full\obj16\Extension.gif"
            bundle="org.modelversioning.emfprofile.edit"/>
      </tools>
    </tools>
    <tools
        xsi:type="gmftool:ToolGroup"
        title="Ecore"
        description=""
        collapsible="true">
      <tools
